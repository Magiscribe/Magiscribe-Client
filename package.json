    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "fix": "pnpm format && pnpm lint",
    "preview": "vite preview",
    "codegen": "graphql-codegen --config codegen.ts"
