    "@headlessui/react": "^2.2.0",
    "@tailwindcss/typography": "^0.5.16",
    "@tailwindcss/vite": "^4.0.2",
    "@xyflow/react": "^12.4.1",
    "clsx": "^2.1.1",
    "globals": "^15.14.0",
    "graphql": "16.10.0",
    "graphql-ws": "6.0.1",
    "motion": "^11.18.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-image-crop": "^11.0.7",
