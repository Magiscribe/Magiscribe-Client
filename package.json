    "@tailwindcss/typography": "^0.5.16",
    "@tailwindcss/vite": "^4.1.5",
    "@xyflow/react": "^12.6.0",
    "clsx": "^2.1.1",
    "dayjs": "^1.11.13",
    "globals": "^16.0.0",
    "graphql": "16.11.0",
    "graphql-ws": "6.0.4",
    "motion": "^12.9.4",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-image-crop": "^11.0.10",
